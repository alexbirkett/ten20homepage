section.mapbox
  #map
    script.
      var callMap = !{JSON.stringify(section.map)};
      $(function() { new ten20.HomeMap('map', callMap); });

    .call-to-action
      h1= section.text[0]
      h2= section.text[1]
      a.button(model-form, src-model="form.contact.data",
        tpl-url="{{form.contact.tplUrl}}",
        method="{{form.contact.method}}",
        path="{{form.contact.path}}",
        redirect="{{form.contact.redirect}}",
        dialog-class="{{form.contact.class}}" 
      ) get started now

    img.banner(src="/images/banner.png")
