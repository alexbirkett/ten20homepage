{
  "base-form": {
    "type": "base-form",
    "logo": "logo.png",
    "name": "simple-form",
    "success-message": "We'll be in touch soon!",
    "require-message": "Please fill in the required fields!",
    "error-message": "Oops! Network connection failed, please try again later!",
    "items": [
    {
      "type": "text",
      "name": "first_name",
      "label": "First name",
      "optional": false
    },
    {
      "type": "text",
      "name": "last_name",
      "label": "Last name",
      "optional": false
    },
    {
      "type": "text",
      "name": "email",
      "label": "Email",
      "optional": false
    }]
  },
  "contact-form": {
    "type": "form",
    "extends": "simple-form",
    "description": "Thank you for your interest, please enter your details below",
    "name": "contact-us",
    "ajaxUrl": "/contact",

    "items": [
    {
      "type": "text",
      "name": "phone",
      "label": "Phone",
      "optional": true
    },
    {
      "type": "text",
      "name": "company_name",
      "label": "Company Name",
      "optional": true
    },
    {
      "type": "text",
      "name": "company_website_url",
      "label": "Your website",
      "optional": true
    },
    {
      "type": "checkbox",
      "name": "call_me",
      "label": "Please call me about ten20",
      "optional": true
    },
    {
      "type": "checkbox",
      "name": "news_letter",
      "label": "Subscribe to partner newsletter",
      "optional": true
    }
    ]

  },
  "free-form": {
    "type": "form",
    "extends": "simple-form",
    "description": "Alex and XiaoLei are working hard on the free plan! Please enter your details below to be notified when it's ready.",
    "name": "free-plan",
    "ajaxUrl": "/signup",
    "redirect": "/user",
    "items": []
  },
  "family-form": {
    "type": "form",
    "extends": "simple-form",
    "ajaxUrl": "/signup",
    "description": "Alex and XiaoLei are working hard on the family plan! Please enter your details below to be notified when it's ready.",
    "name": "family-plan",
    "ajaxUrl": "/signup",
    "redirect": "/user",
    "redirect": "/user",
    "items": []
  },
  "enterprise-form": {
    "type": "form",
    "extends": "simple-form",
    "description": "Alex and XiaoLei are working hard on the enterprise plan! Please enter your details below to be notified when it's ready.",
    "name": "enterprise-plan",
    "ajaxUrl": "/signup",
    "redirect": "/user",
    "items": []
  },
  "signin-form": {
    "type": "form",
    "logo": "logo.png",
    "button_text": "sign in",
    "description": "Sign in to your ten20 account.",
    "name": "signin",
    "ajaxUrl": "/signin",
    "redirect": "/user",
    "items": [
      {
        "type": "text",
        "name": "email",
        "label": "Email",
        "optional": false
      },
      {
        "type": "text",
        "name": "password",
        "label": "Password",
        "optional": false
      },
      {
        "type": "checkbox",
        "name": "remember",
        "label": "Remember me",
        "optional": true
      }
    ]
  }
}
